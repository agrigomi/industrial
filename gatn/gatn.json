{
	"server": [
		{
			"name":		"server-0",
			"port":		8080,
			"buffer":	16384,
			"threads":	8,
			"connections":	2000,
			"timeout":	10,
			"cache": {
				"path":		"/tmp/",
				"key":		"server-0"
			},
			"root":		"../test/AdminLTE",
			"vhost": [
				{
					"host":		"oland.ddns.net:8080",
					"root":		"../test/elaadmin-master",
					"exclude":	"/folder1/:/folder2/",
					"cache": {
						"path":		"/tmp/",
						"key":		"oland"
					},
					"attach": [
						{
							"cname":	"cHttpLog",
							"options":	"-o --lopt=something"
						}
					]
				}
			],
			"ssl": {
				"enable":	false,
				"method":	"SSLv23",
				"flags":	["NO_SSLv2", "NO_SSLv3", "NO_COMPRESSION"],
				"certificate":	"certificate.crt",
				"key":		"keyfile.key"
			},
			"attach":	[
				{
					"cname":	"cHttpLog",
					"options":	""
				}
			]
		}
	],
	"extension": [
		{ "module":	"extgatnmon.so", 	"alias":	"gatn-mon" }
	]
}
