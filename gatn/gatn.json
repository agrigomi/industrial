{
	"server": [
		{
			"name":		"server-0",
			"port":		8443,
			"buffer":	16,
			"threads":	8,
			"connections":	2000,
			"timeout":	10,
			"cache": {
				"path":		"/tmp/",
				"key":		"server-0"
			},
			"root":		"../test/AdminLTE",
			"vhost": [
				{
					"host":		"oland.ddns.net",
					"root":		"../test/elaadmin-master",
					"exclude":	"/folder1/:/folder2/",
					"cache": {
						"path":		"/tmp/",
						"key":		"oland"
					},
					"attach": [
						{
							"cname":	"cHttpLog",
							"options":	"--time-format='%Y-%m-%d %H:%M:%S' -IET"
						},
						{	"cname":	"cAms",
							"options":	"--db-host=localhost --db-user=ams --db-pass=nopass --db-name=ams"
						}
					]
				}
			],
			"ssl": {
				"enable":	true,
				"method":	"SSLv23",
				"certificate":	"example.crt",
				"key":		"example.key"
			},
			"attach":	[
				{
					"cname":	"cHttpLog",
					"options":	"--time-format='%Y-%m-%d %H:%M:%S' -IET"
				},
				{	"cname":	"cAms",
					"options":	"--db-host=localhost --db-user=ams --db-pass=nopass --db-name=ams"
				}
			]
		}
	],
	"extension": [
		{ "module":	"extgatnmon.so", 	"alias":	"gatn-mon" },
		{ "module":	"extgatnams.so",	"alias":	"gatn-ams" }
	]
}
