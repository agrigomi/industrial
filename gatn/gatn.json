{
	"server": [{
			"name":		"server-0",
			"port":		8080,
			"buffer":	16384,
			"threads":	32,
			"connections":	1000,
			"timeout":	10,
			"cache": {
				"path":		"/tmp/",
				"key":		"server-0",
				"exclude":	["folder1", "folder2"]
			},
			"root":		"../test/balay",
			"vhost": [{
					"host":		"oland.ddns.net",
					"root":		"../test/NiceAdmin",
					"cache": {
						"exclude":	["fldr1", "fldr2"],
						"path":		"/tmp/",
						"key":		"oland"
					},
					"attach": [
						{
							"cname":	"cGatnExtClass",
							"options":	"-o --lopt=something"
						}
					]
				},
				{
					"host":		"agrigomi.ddns.net",
					"root":		"../test/StarAdmin",
					"cache": {
						"exclude":	["fldr1", "fldr2"],
						"path":		"/tmp/",
						"key":		"agrigomi"
					}
				}
			],
			"ssl": {
				"enable":	false,
				"method":	"SSLv23",
				"flags":	["NO_SSLv2", "NO_SSLv3", "NO_COMPRESSION"],
				"certificate":	"certificate.crt",
				"key":		"keyfile.key"
			},
			"attach":	[
				{
					"cname":	"cGatnExtClass",
					"options":	"--opt1 --opt2=value"
				}
			]
		},
		{
			"name":		"server-1",
			"port":		8081,
			"buffer":	8192,
			"threads":	32,
			"connections":	1000,
			"timeout":	10,
			"cache": {
				"path":		"/tmp/",
				"key":		"server-1"
			},
			"root":		"../test/philosophy-colorlib",
			"ssl": {
				"enable":	false,
				"method":	"SSLv23",
				"flags":	["NO_SSLv2", "NO_SSLv3", "NO_COMPRESSION"],
				"certificate":	"certificate.crt",
				"key":		"keyfile.key"
			}
		}
	],
	"extensions": [
		{ "module":	"extsample.so", 	"alias":	"example" }
	]
}
