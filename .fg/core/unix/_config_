OUTDIR=bin
COMPILER=gcc
LINKER=gcc
COMPILER_OUTFLAG=-o
OUTSUFFIX=.o
DEPSUFFIX=.d
MAKE_FLAGS=-s --no-print-directory 
SRCSUFFIX=.cpp
PREPROCESSOR=gcc
PREPROCESSOR_OUTFLAG=-MT
LINKER_OUTFLAG=-o
INCLUDES=-Icore/include -Icore/interface
COMPILER_FLAGS=-c -Wall -D_CORE_ $(INCLUDES)
LINKER_FLAGS=--shared -lstdc++
PREPROCESSOR_FLAGS=-MM -Wall -D_CORE_ $(INCLUDES)
